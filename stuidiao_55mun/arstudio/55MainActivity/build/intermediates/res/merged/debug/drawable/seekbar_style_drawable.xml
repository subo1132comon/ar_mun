<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="3.0dip" />
            <corners android:radius="0.0dip" />
            <gradient android:startColor="@color/color_backgroud" android:endColor="@color/color_backgroud" android:angle="45.0" android:centerY="0.75" android:centerColor="@color/color_backgroud" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <size android:height="3.0dip" />
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/color_second_progress" android:endColor="@color/color_second_progress" android:angle="45.0" android:centerY="0.75" android:centerColor="@color/color_second_progress" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="3.0dip" />
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/color_progress" android:endColor="@color/color_progress" android:angle="45.0" android:centerY="0.75" android:centerColor="@color/color_progress" />
            </shape>
        </clip>
    </item>
</layer-list>
